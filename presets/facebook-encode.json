{
  "name": "Facebook Encoding",
  "description": "Optimized settings for Facebook uploads",
  "command": "-i ${INPUT_FILE} -c:v libx264 -preset medium -crf 18 -c:a aac -b:a 192k -vf \"scale=1920:1080,format=yuv420p\" -movflags +faststart ${OUTPUT_FILE}",
  "outputFile": "${INPUT_FILE_DIR}/${INPUT_FILE_BASENAME}.mp4"
}
