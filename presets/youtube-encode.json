{
  "name": "YouTube Encoding",
  "description": "High-quality H.264 encoding for YouTube uploads",
  "command": "-i ${INPUT_FILE} -c:v libx264 -preset medium -crf 16 -c:a aac -b:a 192k -movflags +faststart -pix_fmt yuv420p ${OUTPUT_FILE}",
  "outputFile": "${INPUT_FILE_DIR}/${INPUT_FILE_BASENAME}.mp4"
}
